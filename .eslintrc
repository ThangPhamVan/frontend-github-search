{
    "extends": [
      "prettier",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "prettier",
      "react",
      "@typescript-eslint"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jasmine": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "comma-dangle": 0,
      "react/prop-types": 0,
      "react/jsx-uses-vars": 1,
      "react/display-name": 1,
      "no-unused-vars": 1,
      "no-console": 1,
      "no-undef": 0,
      "no-unexpected-multiline": 1,
      "prettier/prettier": [
        1
      ]
    },
    "globals": {
      "JSX": true,
      "Promise": "readonly",
      "ReturnType" : "readonly"
    }
  }